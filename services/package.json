    "@octokit/rest": "^21.1.1",
    "@openai/codex-sdk": "^0.63.0",
    "@webedt/shared": "file:../shared",
    "bcrypt": "^5.1.1",
    "better-sse": "^0.13.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "form-data": "^4.0.1",
