{
  "name": "@webedt/autonomous-dev-cli",
  "version": "0.1.0",
  "description": "Autonomous development CLI for continuous website improvement",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "autonomous-dev": "./bin/autonomous-dev.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "node --import tsx --test src/**/*.test.ts",
    "test:coverage": "node --import tsx --test --experimental-test-coverage src/**/*.test.ts"
  },
  "keywords": [
    "autonomous",
    "development",
    "cli",
    "claude",
    "ai"
  ],
  "author": "ETdoFresh",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.55",
    "@octokit/rest": "^21.1.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "ora": "^8.2.0",
    "pg": "^8.16.0",
    "simple-git": "^3.27.0",
    "zod": "^3.25.34"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/pg": "^8.15.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
